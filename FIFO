#include<iostream>
using namespace std;
int main()
{
int seq[100];
int memory[4];
int pfs[100];
int found[100];
int i,j,n,t=0;
cout<<"enter the total number of pages in the sequence"<<endl;
cin>>n;
	for(i=0;i<100;i++)
	{
		found[i]=0;
		pfs[t]=0;
	}
	for(i=0;i<n;i++)
	{
		cin>>seq[i];
	}
	/*for(i=0;i<4;i++)
	{ 	
		memory[i]=seq[i];
		
	}*/
	for(i=0;i<n;i++)
	{
		for(j=0;j<4;j++)
		{
			if(seq[i]==memory[j])
			{
				cout<<"No page fault"<<endl;
				found[i]=1;
				break;
			}
		}
		if(found[i]==0)
		{
			memory[t%4]=seq[i];
			cout<<t+1<<"th page fault found for the page "<<seq[i]<<endl;
			pfs[t]=seq[i];
			t++;
		}

	}
	cout<<"The page fault sequence is as follows"<<endl;
	for(i=0;i<t;i++)
	{	
		cout<<pfs[i]<<","<<endl;
	}
return 0;
}
