#include<iostream>
#include<vector>
#include<sys/types.h>
#include<stdio.h>
using namespace std;
vector<int> process;
vector<int> frames;
int main()
{	
	int n,ps,temp,flag=0,count=0;
	cout<<"Enter the number of frames : ";
	cin>>n;
	cout<<"Enter the number of pages to be served : ";
	cin>>ps;
	cout<<"Enter the order of pages : "<<endl;
	for(int i=0;i<ps;i++)
	{
		cout<<"Enter page no : ";
		cin>>temp;
		process.push_back(temp);
	}
	for(int i=0;i<process.size();i++)
	{
		for(int j=0;j<frames.size();j++)
		{
			if(process[i]==frames[j])
			{
				flag=1;
				frames.erase(frames.begin()+j);
				frames.push_back(process[i]);
				break;
			}
		}
		if(flag!=1)
		{
			if(frames.size()!=0 && frames.size()==n)
				frames.erase(frames.begin());
			frames.push_back(process[i]);
			count++;
		}
		flag=0;
		for(int k=0;k<frames.size();k++)
		{
			cout<<frames[k]<<" ,";
		}
		cout<<endl;

	}
	cout<<"Page Conflicts : "<<count<<endl;
	return 0;
}


